#include <iostream>
#include<bits/stdc++.h>
using namespace std;

int main()
{
    int arr[5];
    int n = 5;
    int key,flag =0;
    
    cout<<"enter elements :";
    for(int i=0;i<n;i++)
    {
        cin>>arr[i];
    }
    cout<<"Enter key to be searched :";
    cin>>key;
    //Linear search
    for(int i=0;i<n;i++)
    {
        if(arr[i] == key)
        {
            cout<<"\nKey found";
            flag = 1;
            break;
        }
    }
    if(flag==0)
        cout<<"\nKey not found";

    return 0;
}
